<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:c="http://www.springframework.org/schema/c" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<context:component-scan base-package="com.codingKnowledge.entity" />

	<bean id="e1" class="com.codingKnowledge.entity.Engine">
		<property name="engineName" value="BMW" />
	</bean>

	<bean id="e2" class="com.codingKnowledge.entity.Engine">
		<property name="engineName" value="Audi" />
	</bean>

	<bean id="e3" class="com.codingKnowledge.entity.Engine">
		<property name="engineName" value="Benz" />
	</bean>

</beans>